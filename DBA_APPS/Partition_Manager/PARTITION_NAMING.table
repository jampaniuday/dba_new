CREATE TABLE partition_naming (SCHEMA_NAME VARCHAR2(30), TABLE_NAME VARCHAR2(30), PREFIX VARCHAR2(19));
alter table PARTITION_NAMING
  add constraint UNIQUE_TABLE unique (SCHEMA_NAME, TABLE_NAME);

alter table PARTITION_NAMING
  add constraint UNIQUE_prefix unique (PREFIX);	

CREATE PUBLIC SYNONYM 	PARTITION_NAMING FOR PARTITION_NAMING;

GRANT SELECT ON PARTITION_NAMING TO PUBLIC;
