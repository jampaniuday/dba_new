CREATE TABLE GS_COLUMNS
(
  OWNER               VARCHAR2(30),
  TABLE_NAME          VARCHAR2(30) NOT NULL,
  COLUMN_NAME         VARCHAR2(30) NOT NULL,
  IS_CLEAN_HISTOGRAMS NUMBER(1)    NOT NULL,
  ORDER_SEQUENCE      NUMBER
)
/

GRANT SELECT ON GS_COLUMNS TO PUBLIC
/

begin
  EXECUTE IMMEDIATE 'CREATE PUBLIC SYNONYM GS_COLUMNS FOR GS_COLUMNS';
exception
  when OTHERS then
    null;
end;
/
