CREATE TABLE GS_TABLES
(
  OWNER               VARCHAR2(60),
  TABLE_NAME          VARCHAR2(90) NOT NULL,
  IS_CLEAN_HISTOGRAMS NUMBER(1)    NOT NULL,
  ORDER_SEQUENCE      NUMBER
)
/

GRANT SELECT ON GS_TABLES TO PUBLIC
/

begin
  EXECUTE IMMEDIATE 'CREATE PUBLIC SYNONYM GS_TABLES FOR GS_TABLES';
exception
  when OTHERS then
    null;
end;
/
