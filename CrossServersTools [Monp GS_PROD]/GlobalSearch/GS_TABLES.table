CREATE TABLE GS_TABLES
(ID NUMBER(30) PRIMARY KEY,
 TABLE_NAME VARCHAR2(30) NOT NULL,
 RESULT_VIEW VARCHAR(30) NOT NULL,
 RETURN_ROW_LIMIT NUMBER(10) NOT NULL,
 RESULT_HEADER VARCHAR(4000),
 RESULT_FOOTER VARCHAR(4000),
 DESCRIPTION       VARCHAR2(100));
 
create UNIQUE index tbl_name_idx on GS_TABLES (TABLE_NAME,RESULT_VIEW,RETURN_ROW_LIMIT);