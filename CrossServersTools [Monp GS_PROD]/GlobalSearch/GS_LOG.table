CREATE TABLE GS_LOG
( ID             NUMBER(30) PRIMARY KEY,
  SEARCH_UID     NUMBER(30) NOT NULL,
  SEARCH_TYPE    NUMBER(30),
  TASK_ID        NUMBER(30),
  ACTION         VARCHAR2(30 BYTE),
  TIME_STAMP     DATE DEFAULT SYSDATE,
  SEARCH_STRING  VARCHAR2(100 BYTE),
  ERROR_MSG      VARCHAR2(4000 BYTE),         
  SEARCH_RESULT  CLOB,
  TASK_QUERY     VARCHAR2(4000 BYTE));