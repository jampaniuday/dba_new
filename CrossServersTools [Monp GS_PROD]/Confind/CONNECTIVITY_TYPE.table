CREATE TABLE CONNECTIVITY_TYPE
(
  ID    NUMBER(2)                               NOT NULL,
  NAME  VARCHAR2(50)                            NOT NULL
);


CREATE UNIQUE INDEX PROTOCOL_NAME_UNIQUE ON CONNECTIVITY_TYPE (NAME);


CREATE UNIQUE INDEX PROTOCOL_PK ON CONNECTIVITY_TYPE (ID);


ALTER TABLE CONNECTIVITY_TYPE ADD (
  CONSTRAINT PROTOCOL_PK
 PRIMARY KEY
 (ID),
  CONSTRAINT PROTOCOL_NAME_UNIQUE
 UNIQUE (NAME));
